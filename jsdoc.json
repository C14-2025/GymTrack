{
  "source": {
    "include": [
      "app",
      "components",
      "hooks",
      "lib"
    ],
    "includePattern": ".+\\.(js|jsx|ts|tsx)$", 
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "plugins": [
    "plugins/markdown",
    "@jsdoc/plugin-typescript" 
  ],
  "opts": {
    "encoding": "utf8",
    "destination": "./docs",
    "recurse": true
  }
}
