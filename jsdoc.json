{
  "source": {
    "include": ["app", "components", "hooks", "lib", "scripts", "styles"],
    "includePattern": ".+\\.(js|jsx|ts|tsx)$",
    "excludePattern": "(node_modules|dist)"
  },
  "opts": {
    "destination": "./docs",
    "recurse": true,
    "verbose": true,
    "encoding": "utf8",
    "template": "templates/default"
  }
}
